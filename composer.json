{
    "name": "package/mypackage",
    "description": "symfony Skeleton app no bundle",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": { 
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6",
        "symfony/config": "^3.0",
        "symfony/console": "^3.0",
        "symfony/debug": "^3.0",
        "symfony/dependency-injection": "^3.0",
        "symfony/event-dispatcher": "^3.0",
        "symfony/filesystem": "^3.0",
        "symfony/finder": "^3.0",
        "symfony/http-foundation": "^3.0",
        "symfony/http-kernel": "^3.0",
        "symfony/routing": "^3.0",
        "symfony/twig-bridge": "^3.0",
        "symfony/yaml": "^3.0",
        "twig/twig": "^1.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^3.0",
        "symfony/css-selector": "^3.0",
        "symfony/dom-crawler": "^3.0"
    }
}
